# Uncomment the next line to define a global platform for your project
platform :ios, '17.0'

target 'walkbook' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for walkbook

  # Add the Firebase
  pod 'FirebaseAnalytics'
  pod 'FirebaseAuth'
  pod 'FirebaseFirestore'
  pod 'FirebaseFunctions'
  
  pod 'GoogleSignIn'

  pod 'RxKakaoSDKCommon'
  pod 'RxKakaoSDKAuth'
  pod 'RxKakaoSDKUser'

  pod 'naveridlogin-sdk-ios'
  
  pod 'Swinject'
  pod 'RxSwift'
  pod 'RxCocoa'
  
  target 'walkbookTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Firebase/Auth'
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RxTest'
  end

  target 'walkbookUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RxTest'
  end

end
